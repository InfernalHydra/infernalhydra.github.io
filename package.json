  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "animejs": "^3.2.1"
  },
  "devDependencies": {
    "node-sass": "^7.0.1"
  },
  "scripts": {
    "css-build": "node-sass --omit-source-map-url sass -o css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch",
    "copy-libraries": "cp node_modules/@fortawesome/fontawesome-free/js/all.js js/ && cp node_modules/animejs/lib/anime.min.js js/",
    "build": "npm run copy-libraries && npm run css-build"
  },
